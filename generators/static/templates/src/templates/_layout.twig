<!doctype html>
<html lang=en>
<head>
<meta charset=utf-8>
<meta name="viewport" content="width=device-width, initial-scale=1">
{% include '_partials/head.twig' %}
</head>

<body>
<a class="skip-link" href="#main-content">Skip to content</a>

<main class="l-container" tabindex="-1" role="main" id="main-content">
  {% block content %}{% endblock %}
</main>

<script defer src="{{ rootDistPath }}/scripts/main.bundle.js"></script>

<span class="h-hide-visually">
  {% include '_svg/symbol/icon.symbol.svg' ignore missing %}
  {% include '_svg/symbol/full.symbol.svg' ignore missing %}
</span>

<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-XXXXXXXX-X"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-XXXXXXXX-X');
</script>

</body>
</html>
